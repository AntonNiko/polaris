<launch>        
        <!-- Vision Node -->

        <arg name="name" default="top_face_cam"/>
        <arg name="fd" default="/dev/video0"/>

        <node name="camera_one" pkg="vision" type="sender" required="true">
          <param name="name" value="$(arg name)"/>
          <param name="fd" value="$(arg fd)"/>
        </node>

</launch>
